<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1" name="PLSQL Scanner">
    <description>Provides a scanner for PLSQL files.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.plsql.model.PLSQLDescriptor</class>
        <class>org.jqassistant.contrib.plugin.plsql.model.PLSQLFileDescriptor</class>
        <class>org.jqassistant.contrib.plugin.plsql.model.PLSQLBlockDescriptor</class>
        <class>org.jqassistant.contrib.plugin.plsql.model.DeclareBlockDescriptor</class>
        <class>org.jqassistant.contrib.plugin.plsql.model.BeginBlockDescriptor</class>
        <class>org.jqassistant.contrib.plugin.plsql.model.ExceptionBlockDescriptor</class>
        <class>org.jqassistant.contrib.plugin.plsql.model.VariableDescriptor</class>
        <class>org.jqassistant.contrib.plugin.plsql.model.DataTypeDescriptor</class>
        <class>org.jqassistant.contrib.plugin.plsql.model.ValueDescriptor</class>
        <class>org.jqassistant.contrib.plugin.plsql.model.SubprogramDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.plsql.scanner.PLSQLFileScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>